{
  "name": "kaskadi-passwordbox",
  "version": "1.0.0",
  "description": "",
  "main": "kaskadi-passwordbox.js",
  "directories": {
    "example": "example",
    "test": "test"
  },
  "scripts": {
    "example": "xdg-open localhost:1234/example && serve -l 1234",
    "test": "npm run lint && npm run test.All",
    "test.FF": "karma start --single-run --browsers FirefoxHeadless karma.conf.js",
    "test.Chrome": "karma start --single-run --browsers ChromeHeadless karma.conf.js",
    "test.All": "karma start --single-run --browsers ChromeHeadless,FirefoxHeadless karma.conf.js",
    "lint": "standard --fix",
    "coverage": "npm run lint && npm run test.Chrome"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kaskadi/kaskadi-passwordbox.git"
  },
  "keywords": [],
  "author": "Klimapartner GmbH <kontakt@klimapartner.de> (https://klimapartner.de)",
  "contributors": [
    "Holger Will <h.will@klimapartner.de>",
    "Alexis Lemaire <a.lemaire@klimapartner.de>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kaskadi/kaskadi-passwordbox/issues"
  },
  "homepage": "https://github.com/kaskadi/kaskadi-passwordbox#readme",
  "kaskadi": {
    "s3-push": {
      "files": [
        {
          "src": "kaskadi-passwordbox.js",
          "dest": "modules/@kaskadi/kaskadi-passwordbox/{branch}kaskadi-passwordbox.js"
        },
        {
          "src": "example/index.html",
          "dest": "modules/@kaskadi/kaskadi-passwordbox/{branch}example/index.html"
        },
        {
          "src": "font-generator/pwd-font.ttf",
          "dest": "modules/@kaskadi/kaskadi-passwordbox/{branch}pwd-font.ttf"
        },
        {
          "src": "import-font.css",
          "dest": "modules/@kaskadi/kaskadi-passwordbox/{branch}import-font.css"
        }
      ]
    }
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "karma": "^4.4.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.1",
    "karma-firefox-launcher": "^1.3.0",
    "karma-mocha": "^1.3.0",
    "mocha": "^7.0.0",
    "standard": "^14.3.1"
  },
  "dependencies": {}
}
